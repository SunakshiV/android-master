<?xml version="1.0" encoding="utf-8"?>


<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_selected="false">
        <shape android:dither="true" android:shape="rectangle">

            <gradient android:angle="0" android:endColor="#ff876b" android:startColor="#ffbca6" android:type="linear" />
            <corners android:radius="13dip" />
        </shape>

    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">

            <solid android:color="@color/cefefef" />
            <corners android:radius="13dip" />

        </shape>

    </item>

</selector>